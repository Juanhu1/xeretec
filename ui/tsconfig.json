{
    "compileOnSave": true,
    "compilerOptions": {      
      "allowJs": false,
      "outDir":"./js",
      "alwaysStrict": false,
      "target": "es5",
      "noEmitOnError": false,
      "noImplicitAny": false,
      "noImplicitThis": false,
      "strictNullChecks": false,
      "strictFunctionTypes": true,
      "module": "amd",
      "checkJs": false,
      "sourceMap": true,
      "baseUrl": "./src/js",
      "moduleResolution": "node",
      "paths": {
          "ojs/*":["../../node_modules/@oracle/oraclejet/dist/types/*"]
      },
      "lib": ["dom","es5","scripthost", "es2015.core","es2015.collection",
              "es2015","esnext.asynciterable"]
    }
}